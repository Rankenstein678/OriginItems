{
  "schemaVersion": 1,
  "id": "origin_items",
  "version": "${version}",

  "name": "Origin Items",
  "description": "A few items made to expand on the Origins Mod",
  "authors": [
    "Rankenstein"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/origin_items/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.rankenstein.origin_items.OriginItems"
    ],"client": [
      "com.rankenstein.origin_items.client.OriginItemsClient"
    ],"fabric-datagen": [
      "com.rankenstein.origin_items.datagen.ModDataGeneration"
    ]
  },
  "accessWidener": "origin_items.accesswidener",
  "mixins": [
    "origin_items.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "origins": ">=1.7.1",
    "geckolib3": "3.1.38",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}